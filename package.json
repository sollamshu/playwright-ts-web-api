{
  "name": "playwright-ts-web-api",
  "version": "1.0.0",
  "description": "Playwright framework for Web and API testing",
  "main": "index.js",
  "scripts": {
    "test:web": "npx playwright test --project=chromium --grep @web",
    "test:api": "npx playwright test --project=api --grep @api",
    "report": "npx playwright show-report",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,md}\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@playwright/test": "^1.44.1",
    "@types/node": "^20.14.2",
    "dotenv": "^16.4.5",
    "prettier": "^3.5.3"
  }
}
